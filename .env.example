# Required API Keys
GEMINI_API_KEY=your_gemini_api_key
ANTHROPIC_API_KEY=your_anthropic_api_key

# Optional: Recraft API for illustration generation
RECRAFT_API_KEY=your_recraft_api_key

# Screenshot provider: "playwright" (default, for local/Railway) or "screenshotone" (for Vercel)
SCREENSHOT_PROVIDER=playwright

# Only needed if SCREENSHOT_PROVIDER=screenshotone
SCREENSHOTONE_API_KEY=your_screenshotone_api_key
